<root name="ad" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <dynamic cell="user" one="true">
        <element name="user">
            <attribute name="id" cell="id" />
            <attribute name="seor" cell="seor" />
            <attribute name="days" cell="days" />
            <attribute name="clicks" cell="clicks" />
            <attribute name="ads" cell="ads" />
            <attribute name="message" cell="message" />
            <attribute name="notification" cell="notification" />
            <attribute name="employer" cell="employer" />
            <attribute name="took_days" cell="took_days" />
            <attribute name="email" cell="email" />
            <attribute name="logo" cell="logo" />
            <attribute name="no_logo" cell="no_logo" />
            <attribute name="logo_path" cell="logo_path" />
            <attribute name="id_country" cell="id_country" />
            <attribute name="name" cell="name" />
            <attribute name="complete" cell="complete" />
        </element>
    </dynamic>
    <dynamic cell="ads">
        <element name="ads">
            <attribute name="id" cell="id" />
            <attribute name="id_user" cell="id_user" />
            <attribute name="title" cell="title" />
            <attribute name="salary" cell="salary" />
            <attribute name="time" cell="time" />
            <attribute name="status" cell="status" />
            <attribute name="approved" cell="approved" />
            <attribute name="id_country" cell="id_country" />
            <attribute name="id_currency" cell="id_currency" />
            <attribute name="pay" cell="pay" />
            <attribute name="complete" cell="complete" />
            <attribute name="expiration" cell="expiration" />
            <attribute name="expiration_time" cell="expiration_time" />
            <element name="description">
                <text cell="description" />
            </element>
        </element>
    </dynamic>
    <dynamic cell="ads_specialization">
        <element name="ads_specialization">
            <attribute name="id_ads" cell="id_ads" />
            <attribute name="id_specialization" cell="id_specialization" />
        </element>
    </dynamic>
    <dynamic cell="ads_language">
        <element name="ads_language">
            <attribute name="id_ads" cell="id_ads" />
            <attribute name="id_language" cell="id_language" />
        </element>
    </dynamic>
    <dynamic cell="specialization">
        <element name="specialization">
            <attribute name="id" cell="id" />
            <text cell="name" />
        </element>
    </dynamic>
    <dynamic cell="language">
        <element name="language">
            <attribute name="id" cell="id" />
            <text cell="name" />
        </element>
    </dynamic>
    <dynamic cell="currency_name">
        <element name="currency_name">
            <attribute name="id" cell="id" />
            <text cell="name" />
        </element>
    </dynamic>
    <dynamic cell="country">
        <element name="country">
            <attribute name="id" cell="id" />
            <text cell="name" />
        </element>
    </dynamic>
    <dynamic cell="get" one="true">
        <element name="get">
            <dynamic>
                <dynamic>
                    <element name="element" key="last">
                        <text />
                    </element>
                </dynamic>
            </dynamic>
        </element>
    </dynamic>
</root>